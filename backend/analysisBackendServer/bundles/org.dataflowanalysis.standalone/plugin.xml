<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.dataflowanalysis.standalone.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.dataflowanalysis.standalone.application"
            name="Standalone">
         <property
               name="appName"
               value="Standalone">
         </property>
      </product>
   </extension>
   <extension
         id="product1"
         point="org.eclipse.core.runtime.products">
      <product
            application="DataFlowAnalalysisStandalone.application"
            name="TestTest">
         <property
               name="appName"
               value="TestTest">
         </property>
      </product>
   </extension>
</plugin>
